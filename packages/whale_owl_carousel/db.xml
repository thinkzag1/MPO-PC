	    <field name="dateModified" type="T"><default value="0000-00-00 00:00:00"/><notnull/></field>
	    <field name="fmUID" type="I" size="10"><default value="0"/><unsigned/></field>
	    <field name="lmUID" type="I" size="10"><default value="0"/><unsigned/></field>
	</table>
</schema>