			<autoincrement/>
			<unsigned/>
		</field>
		<field name="carouselName" type="C" size="255"></field>
		<field name="carouselPlugin" type="C" size="50"></field>
		<field name="carouselTheme" type="C" size="50"></field>
		<field name="options" type="X2"></field>
		<field name="items" type="X2"></field>
